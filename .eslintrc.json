{
    "env": {
        "es2021": true,
        "browser": true,
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "indent": ["error", "tab"],
        "quotes": ["error", "double"],
        "semi": ["error", "always"],
        "no-multi-spaces": ["error"],
		"no-var": ["error"]
    }
}
